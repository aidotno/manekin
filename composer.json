{
    "name": "matriphe/manekin",
    "description": "Faker extender specific for Indonesian data.",
    "keywords": [
        "faker",
        "fake",
        "fake-data",
        "indonesia",
        "id",
        "test",
        "dummy",
        "manequin",
        "manekin"
    ],
    "type": "library",
    "require": {
        "php": "^5.4 || ^7.0",
        "fzaninotto/faker": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0"
    },
    "suggest": {
        "illuminate/support": "Required to use for Laravel integration (5.*)."
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Zamroni",
            "email": "halo@matriphe.com",
            "homepage": "http://matriphe.com"
        }
    ],
    "autoload":{  
        "psr-4":{  
            "Matriphe\\Manekin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Matriphe\\Manekin\\Tests\\": "tests/",
            "Faker\\Test\\": "vendor/fzaninotto/faker/test/Faker/"
        }
    },
    "config":{  
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Matriphe\Manekin\ManekinServiceProvider"
            ]
        }
    }
}
